# Production Environment Variables

# Database
POSTGRES_USER=ocf_worker
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=ocf_worker_db

# Worker
WORKER_PORT=8081
LOG_LEVEL=info
VERSION=latest

# Storage
STORAGE_TYPE=garage
GARAGE_ENDPOINT=https://your-garage-endpoint.com
GARAGE_ACCESS_KEY=your_access_key
GARAGE_SECRET_KEY=your_secret_key
GARAGE_BUCKET=ocf-courses

# Jobs
JOB_TIMEOUT=45m
CLEANUP_INTERVAL=6h
