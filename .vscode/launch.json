{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch OCF Worker",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/generator",
      "cwd": "${workspaceFolder}",
      "env": {
        "PORT": "8081",
        "LOG_LEVEL": "debug",
        "ENVIRONMENT": "development"
      },
      "args": [],
      "showLog": true,
      "trace": "verbose"
    },
    {
      "name": "Debug Tests",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceFolder}",
      "cwd": "${workspaceFolder}",
      "env": {
        "ENVIRONMENT": "test"
      }
    }
  ]
}